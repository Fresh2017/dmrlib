#ifndef _DMR_CONFIG_H
#define _DMR_CONFIG_H

/* These are auto-generated by configure at compile time */
{% for key, value in env.items()|sort %}{% if key.startswith('HAVE_') %}{% if value == '1' -%}
#define DMR_{{ key }} 1
{% else -%}
/* #undef DMR_{{ key }} */
{% endif %}{% endif %}{% endfor %}

{% if env.HAVE_VISIBILITY_ATTRIBUTE == '1' -%}
#define DMR_API __attribute__((visibility("default")))
#define DMR_PRV __attribute__((visibility("hidden")))
{% elif env.HAVE_VISIBILITY_DECLSPEC == '1' -%}
#define DMR_API __declspec(dllexport)
#define DMR_PRV
{% else -%}
#define DMR_API
#define DMR_PRV
{% endif %}

#endif // _DMR_CONFIG_H
